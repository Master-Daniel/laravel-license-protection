{
    "name": "elite-codec/laravel-license-protection",
    "description": "Bulletproof license protection package for Laravel applications",
    "type": "library",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "license",
        "protection",
        "validation"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/database": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LicenseProtection\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LicenseProtection\\LicenseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

